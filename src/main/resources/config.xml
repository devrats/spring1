<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd">

<bean class="com.example.demo.Student" name="student1">
    <property name="studentID" >
        <value>12</value>
    </property>
    <property name="studentName">
        <value>devvrat</value>
    </property>
    <property name="studentAddress">
        <value>agra</value>
    </property>
</bean>
    <bean class="com.example.demo.Student" name="student2" p:studentID="123" p:studentName="ramu" p:studentAddress="noida"/>
    <bean class="com.example.demo.Student" name="student3">
        <property name="studentAddress" value="delhi"/>
        <property name="studentName" value="shyamu"/>
        <property name="studentID" value="1234"/>
    </bean>
